{
  "name": "proyectos_electron",
  "version": "1.0.0",
  "description": "primeros pasos en electron",
  "main": "main.js",
  "devDependencies": {
    "electron-packager": "^14.1.0",
    "electron": "^7.1.1"
  },
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=ico.png --prune=true --out=release-builds ",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=ico.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a52hepof/electron.git"
  },
  "author": "Fernando y Antonio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/a52hepof/electron/issues"
  },
  "homepage": "https://github.com/a52hepof/electron#readme",
  "dependencies": {
    "electron-reload": "^1.5.0",
    "mysql": "^2.17.1"
  }
}
